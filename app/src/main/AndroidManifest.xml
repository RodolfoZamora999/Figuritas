<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.autism.figuritas">

    <application
        android:name=".MyDatabaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Figuritas">
        <activity
            android:name=".iu.session.RegisterActivity"
            android:screenOrientation="landscape"/>
        <activity
            android:name=".iu.session.SessionActivity"
            android:screenOrientation="landscape"/>
        <activity
            android:name=".iu.levels.LevelActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".iu.config.InitConfigActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".iu.MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>